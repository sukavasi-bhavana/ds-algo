/*
https://leetcode.com/problems/reverse-words-in-a-string/
*/
public class Solution {
    public string ReverseWords(string s) {
        string[] strarr ;
            strarr=s.Trim().Split(' ');
            string res = "";
            for(int i=strarr.Length-1;i>=0;i--)
            {
                if(strarr[i]!="")
                res = res + ' ' + strarr[i];
            }
        return res.Trim();
        
    }
}
