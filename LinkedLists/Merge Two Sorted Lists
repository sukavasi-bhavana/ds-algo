/*
https://leetcode.com/problems/merge-two-sorted-lists/
*/
public ListNode RemoveNthFromEnd(ListNode head, int n) {
       ListNode fast=head;
        while(n!=0)
        {
            fast=fast.next;
            n--;
        }
        ListNode slow=head;
        if(fast==null)
            return head.next;
        while(fast.next!=null)
        {
            fast=fast.next;
            slow=slow.next;
        }
        slow.next=slow.next.next;
        return head;
        
        
    }
